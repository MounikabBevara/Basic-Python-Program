arr=[10,20,30,40,50]
target=int(input("Enter the Target Value:"))
def binary_search(arr,target):
  lb=0
  ub=len(arr)-1
  while lb<=ub:
      mid=(lb+ub//2)
      if(arr[mid]==target):
          return mid
      elif(arr[mid]<target):
          lb=mid+1
      else:
          ub=mid-1
          return -1
result=binary_search(arr,target)
if(result!=-1):
    print("Elements found in the index",result)
else:
    print("Elements not found")
